    "@vue-office/docx": "^1.6.2",
    "@vue-office/excel": "^1.7.11",
    "@vue-office/pdf": "^2.0.2",
    "axios": "^1.7.7",
    "axios-retry": "^4.5.0",
    "docx-preview": "^0.3.2",
    "element-plus": "^2.8.4",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pdfjs-dist": "^4.7.76",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.1",
    "spark-md5": "^3.0.2",
    "vue": "^3.5.11",
    "vue-router": "^4.4.5",
    "vuedraggable": "^4.1.0",
