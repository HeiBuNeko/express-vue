    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "sass": "^1.79.4",
    "typescript": "^5.6.3",
    "unplugin-auto-import": "^0.18.3",
